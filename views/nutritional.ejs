<div class="container">
    <div class="section">

        <style>

        .arc text {
            font: 10px sans-serif;
            text-anchor:middle;
        }

        .arc path {
            stroke: #fff;
        }
        </style>
        
        <svg width="960" height="500"></svg>
        <script src="https://d3js.org/d3.v4.min.js"></script>
        <script>

        var data = [{
            population:2704659,
            age:"<5"
        },
        {
            population:4499890,
            age:"5-13"
        },
        {
            population:2159981,
            age: "14-17"
        },
        {
            population:3853788,
            age: "18-24"
        },
        {
            population:14106543,
            age: "25-44"
        },
        {
            population:8819342,
            age:"45-64"
        },
        {
            population:612463,
            age:"≥65"
        }];


            var svg = d3.select("svg"),
            width = +svg.attr("width"),
            height = +svg.attr("height"),
            radius = Math.min(width,height) / 2,
            g = svg.append("g").attr("transform","translate(" + width / 2 + "," + height / 2 + ")");
        
            var color = d3.scaleOrdinal(["#98abc5","#8a89a6","#7b6888","#6b486b","#a05d56","#d0743c","#ff8c00"]);

//                 <5,2704659
// 5-13,4499890
// 14-17,2159981
// 18-24,3853788
// 25-44,14106543
// 45-64,8819342
// ≥65,612463

            var pie = d3.pie()
                .sort(null)
                .value((d) => d.population);

            var path = d3.arc()
                .outerRadius(radius - 10)
                .innerRadius(0);

            var label = d3.arc()
                .outerRadius(radius - 40)
                .innerRadius(radius - 40);

            var arc = g.selectAll(".arc")
                .data(pie(data))
                .enter().append("g")
                    .attr("class","arc");

            arc.append("path")
                .attr("d",path)
                .attr("fill", (d) => {
                    return color(d.data.age);
                });

            arc.append("text")
                .attr("transform", (d) => { return "translate(" + label.centroid(d) + ")"; })
                .attr("dy","0.35em")
                .text((d) => { return d.data.age; })

        </script>
    </div>
</div>